                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "bg-white border border-gray-200 rounded-lg p-4",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.FlagIcon, {
                                    className: "h-6 w-6 text-blue-400"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "ml-3",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-sm font-medium text-gray-500",
                                            children: "Total Flags"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-lg font-semibold text-gray-900",
                                            children: Object.keys(hierarchy.effectiveFlags).length
                                        })
                                    ]
                                })
                            ]
                        })
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "bg-white border border-gray-200 rounded-lg p-4",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.CheckCircleIcon, {
                                    className: "h-6 w-6 text-green-400"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "ml-3",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-sm font-medium text-gray-500",
                                            children: "Enabled"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-lg font-semibold text-gray-900",
                                            children: Object.values(hierarchy.effectiveFlags).filter((f)=>f.enabled).length
                                        })
                                    ]
                                })
                            ]
                        })
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "bg-white border border-gray-200 rounded-lg p-4",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.XCircleIcon, {
                                    className: "h-6 w-6 text-red-400"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "ml-3",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-sm font-medium text-gray-500",
                                            children: "Disabled"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-lg font-semibold text-gray-900",
                                            children: Object.values(hierarchy.effectiveFlags).filter((f)=>!f.enabled).length
                                        })
                                    ]
                                })
                            ]
                        })
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "bg-white border border-gray-200 rounded-lg p-4",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.ExclamationTriangleIcon, {
                                    className: "h-6 w-6 text-yellow-400"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "ml-3",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-sm font-medium text-gray-500",
                                            children: "Inherited"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-lg font-semibold text-gray-900",
                                            children: hierarchy.inheritance_chain.length
                                        })
                                    ]
                                })
                            ]
                        })
                    })
                ]
            }),
            !selectedModuleId && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "text-center py-12",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.CubeIcon, {
                        className: "mx-auto h-12 w-12 text-gray-400"
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                        className: "mt-2 text-sm font-medium text-gray-900",
                        children: "Select a Module"
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                        className: "mt-1 text-sm text-gray-500",
                        children: "Choose a module above to view its feature flag hierarchy."
                    })
                ]
            })
        ]
    });
};

