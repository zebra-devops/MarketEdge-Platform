                                                    })
                                                ]
                                            })
                                        })
                                    ]
                                }),
                                showErrors && (previewData.errors.length > 0 || previewData.duplicates.length > 0) && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "space-y-4",
                                    children: [
                                        previewData.errors.length > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                            className: "border border-red-200 rounded-lg",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                    className: "bg-red-50 px-4 py-2 border-b border-red-200",
                                                    children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("h4", {
                                                        className: "font-medium text-red-900",
                                                        children: [
                                                            "Validation Errors (",
                                                            previewData.errors.length,
                                                            ")"
                                                        ]
                                                    })
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                    className: "max-h-40 overflow-y-auto",
                                                    children: previewData.errors.map((error, index)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                            className: "px-4 py-2 border-b border-red-100 last:border-b-0",
                                                            children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                                className: "text-sm",
                                                                children: [
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                        className: "font-medium text-red-900",
                                                                        children: [
                                                                            "Row ",
                                                                            error.row,
                                                                            ":"
                                                                        ]
                                                                    }),
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                                        className: "text-red-700 ml-2",
                                                                        children: sanitizeHTML(error.message)
                                                                    }),
                                                                    error.field && /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                        className: "text-red-600 ml-2",
                                                                        children: [
                                                                            "(",
                                                                            sanitizeHTML(error.field),
                                                                            ")"
                                                                        ]
                                                                    })
                                                                ]
                                                            })
                                                        }, index))
                                                })
                                            ]
                                        }),
                                        previewData.duplicates.length > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                            className: "border border-yellow-200 rounded-lg",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                    className: "bg-yellow-50 px-4 py-2 border-b border-yellow-200",
                                                    children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("h4", {
                                                        className: "font-medium text-yellow-900",
                                                        children: [
                                                            "Duplicate Emails (",
                                                            previewData.duplicates.length,
                                                            ")"
                                                        ]
                                                    })
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                    className: "max-h-40 overflow-y-auto",
                                                    children: previewData.duplicates.map((duplicate, index)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                            className: "px-4 py-2 border-b border-yellow-100 last:border-b-0",
                                                            children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                                className: "text-sm",
                                                                children: [
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                        className: "font-medium text-yellow-900",
                                                                        children: [
                                                                            "Row ",
                                                                            duplicate.row,
                                                                            ":"
                                                                        ]
                                                                    }),
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                                        className: "text-yellow-700 ml-2",
                                                                        children: sanitizeHTML(duplicate.email)
                                                                    }),
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                        className: "text-yellow-600 ml-2",
                                                                        children: [
                                                                            "- ",
                                                                            sanitizeHTML(duplicate.message)
                                                                        ]
                                                                    })
                                                                ]
                                                            })
                                                        }, index))
                                                })
                                            ]
                                        })
                                    ]
                                })
                            ]
                        }),
                        importBatch && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                            className: "space-y-4",
                            children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "bg-white border rounded-lg p-4",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "flex items-center justify-between mb-4",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                className: "flex items-center space-x-2",
                                                children: [
                                                    getStatusIcon(importBatch.status),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("h4", {
                                                        className: "font-medium text-gray-900",
                                                        children: "Import Progress"
                                                    })
                                                ]
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "text-sm text-gray-500 capitalize",
                                                children: importBatch.status
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "mb-4",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                className: "flex justify-between text-sm text-gray-600 mb-1",
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                        children: "Progress"
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                        children: [
                                                            getProgressPercentage(),
                                                            "%"
                                                        ]
                                                    })
                                                ]
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "w-full bg-gray-200 rounded-full h-2",
                                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                    className: "bg-blue-600 h-2 rounded-full transition-all duration-300",
                                                    style: {
                                                        width: `${getProgressPercentage()}%`
                                                    }
                                                })
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "grid grid-cols-2 md:grid-cols-4 gap-4 text-center",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-lg font-semibold text-gray-900",
                                                        children: importBatch.total_rows
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-sm text-gray-500",
                                                        children: "Total"
                                                    })
                                                ]
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-lg font-semibold text-green-600",
                                                        children: importBatch.successful_rows
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-sm text-gray-500",
                                                        children: "Success"
                                                    })
                                                ]
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-lg font-semibold text-red-600",
                                                        children: importBatch.failed_rows
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-sm text-gray-500",
                                                        children: "Failed"
                                                    })
                                                ]
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-lg font-semibold text-blue-600",
                                                        children: importBatch.processed_rows
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                        className: "text-sm text-gray-500",
                                                        children: "Processed"
                                                    })
                                                ]
                                            })
                                        ]
                                    }),
                                    importBatch.error_message && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                        className: "mt-4 p-3 bg-red-50 border border-red-200 rounded-md",
                                        children: /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                            className: "text-sm text-red-700",
                                            children: importBatch.error_message
                                        })
                                    })
                                ]
                            })
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex justify-end space-x-3 pt-4 border-t",
                            children: [
                                importBatch?.status === "completed" || importBatch?.status === "failed" ? /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.default, {
                                    onClick: resetForm,
                                    className: "bg-indigo-600 hover:bg-indigo-700 text-white",
                                    children: "Import Another File"
                                }) : null,
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.default, {
                                    onClick: handleClose,
                                    variant: "secondary",
                                    children: importBatch ? "Close" : "Cancel"
                                })
                            ]
                        })
                    ]
                })
            })
        ]
    });
}

